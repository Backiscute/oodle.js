{
    "name": "oodle.js",
    "version": "1.0.0",
    "description": "Oodle compression wrapper in JS.",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "bin"
    ],
    "scripts": {
        "build": "tsup",
        "lint": "prettier --check ./src && eslint ./src",
        "check": "tsc --noEmit",
        "format": "prettier --write ./src && eslint ./src --fix",
        "test": "vitest run",
        "coverage": "vitest run --coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/backiscute/oodle.js.git"
    },
    "author": {
        "name": "Back",
        "email": "me@back.rs",
        "url": "https://back.rs"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/backiscute/oodle.js/issues"
    },
    "homepage": "https://github.com/backiscute/oodle.js#readme",
    "keywords": [
        "oodle",
        "compression",
        "decompression",
        "unreal engine",
        "unrealengine",
        "ue4",
        "ue5",
        "fortnite",
        "valorant"
    ],
    "dependencies": {
        "koffi": "^2.14.0",
        "lzma-native": "^8.0.6",
        "uzip": "^0.20201231.0"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.27.1",
        "@backs/tsconfig": "^1.0.5",
        "@eslint/js": "^9.35.0",
        "@stylistic/eslint-plugin": "^5.3.1",
        "@types/node": "^24.3.1",
        "@types/uzip": "^0.20201231.2",
        "@vitest/coverage-v8": "^3.2.4",
        "babel-plugin-transform-import-meta": "^2.3.3",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "prettier": "^3.6.2",
        "tsup": "^8.5.0",
        "tsx": "^4.20.5",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.42.0",
        "vitest": "^3.2.4"
    }
}
